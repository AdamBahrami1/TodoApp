{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Node.js",
        "program": "${workspaceFolder}/src/App.js",
        "runtimeArgs": ["--experimental-modules"],
        "outFiles": ["${workspaceFolder}/out"],
        "stopOnEntry": false,
        "args": [],
        "cwd": "${workspaceFolder}",
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal" // Use "integratedTerminal" for integrated terminal or "externalTerminal" for an external terminal
      }
    ]
  }
  